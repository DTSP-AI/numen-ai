# Numen AI - Environment Variables Template
# Copy this file to .env and fill in your actual values

# === SUPABASE CONFIGURATION ===
# Get these from your Supabase project dashboard

# Supabase Database Connection (use Connection Pooling URL)
# Settings → Database → Connection String → Transaction mode
SUPABASE_DB_URL=postgresql://postgres.xxx:[YOUR-PASSWORD]@xxx.pooler.supabase.com:5432/postgres

# Supabase API URLs
# Settings → API → Project URL
SUPABASE_URL=https://xxx.supabase.co

# Supabase API Key (anon/public key)
# Settings → API → Project API keys → anon public
SUPABASE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# === LLM CONFIGURATION ===
OPENAI_API_KEY=sk-proj-...

# === LIVEKIT (Voice/Video) ===
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=APIxxxxxxxxx
LIVEKIT_API_SECRET=your-secret-here

# === SPEECH SERVICES ===
# Deepgram (Speech-to-Text)
DEEPGRAM_API_KEY=your-deepgram-key

# ElevenLabs (Text-to-Speech)
ELEVENLABS_API_KEY=your-elevenlabs-key

# === MEMORY & VECTOR STORAGE ===
# Qdrant (runs locally via Docker or cloud)
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=  # Optional for cloud Qdrant

# Mem0 Configuration
MEM0_PROJECT=agentic-os
MEM0_STORE=local

# === SESSION MANAGEMENT ===
# Redis (runs locally via Docker or cloud)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=  # Optional for cloud Redis

# === APP CONFIGURATION ===
ENVIRONMENT=development
LOG_LEVEL=INFO
PORT=8000
DEBUG=false

# === OPTIONAL: Local PostgreSQL Fallback ===
# Only used if SUPABASE_DB_URL is not set
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_USER=hypnoagent
# POSTGRES_PASSWORD=
# POSTGRES_DB=hypnoagent
